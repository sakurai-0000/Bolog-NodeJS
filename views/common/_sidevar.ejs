 <!-- sidevar(right) menber list -->
<div class="sidenav">
    <% for(const group in data) { %>
        <p><%- include('../function/_getGroupName', {group: group}); %></p>
        <% for (const gene in data[group]) { %>
            <% data[group][gene].map((x) => { %>
                <a href="/profile?grp=<%= group %>&gene=<%= gene %>&num=<%= x.num %>"><%= x.name_jp %></a>
            <% }); %>
        <% } %>
    <% } %>
</div>
