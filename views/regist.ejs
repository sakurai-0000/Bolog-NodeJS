<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Hinata</title>
    <link rel="stylesheet" type="text/css" href="../public/css/main.css">
</head>

<body>
    <%- include('./common/_header', null) %>
    <div align="center">
        <form action="/upload" method="POST">
            <table>
                <tr>
                    <td>グループ名</td>
                    <td><input type="text" name="grp"></td>
                </tr>
                <tr>
                    <td>期</td>
                    <td><input type="text" name="gene"></td>
                </tr>
                <tr>
                    <td>名前</td>
                    <td><input type="text" name="name_jp"></td>
                </tr>
                <tr>
                    <td>生年月日</td>
                    <td><input type="text" name="birth"></td>
                </tr>
                <tr>
                    <td>星座</td>
                    <td><input type="text" name="star"></td>
                </tr>
                <tr>
                    <td>身長</td>
                    <td><input type="text" name="height"></td>
                </tr>
                <tr>
                    <td>出身地</td>
                    <td><input type="text" name="birthplace"></td>
                </tr>
                <tr>
                    <td>血液型</td>
                    <td><input type="text" name="blood"></td>
                </tr>
            </table>
            <input type="submit" value="Join!">
        </form>
    </div>
    <%- include('./common/_footer', null) %>
</body>
</html>